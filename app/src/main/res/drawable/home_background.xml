<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Background Gradient -->
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:startColor="#0209f9"
                android:endColor="#9f02f9"
                android:angle="10"/>

        </shape>
    </item>
    <!-- Border -->
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="2dp"
                android:color="#f3ff33"/>
        </shape>
    </item>
</layer-list>
